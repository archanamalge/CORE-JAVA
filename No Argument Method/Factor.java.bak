class Factor 
{
	public static void main(String[] args) 
	{
		fact();
		
	}

	public static void fact()
	{
		int num=100;
		int sum=0;
		for(int i=1;i<num;i++)
		{
			if((num/2)%i==0)
			{
				System.out.println(i);
				sum+=num;
			}
		}
		System.out.println("The Sum is : "+sum);
	}
}
