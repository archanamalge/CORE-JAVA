import java.util.Scanner;
class FermetNum 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the Number : ");
		int num=sc.nextInt();
	}
	public static int pow(int num)
	{
		int count=2;
		int pow=1;
		for(int i=1;i<=count;i++)
		{
			pow*=i;
		}
		return pow;
	}

